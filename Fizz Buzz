using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
            int x;
            string y = "Fizz";
            for (x = 0; x <= 100; x++)
            {
                System.Console.WriteLine(x);
            }
            System.Console.ReadLine();
            
            if (x % 3 == 0)
            {
                System.Console.WriteLine(y);
                System.Console.ReadLine();
                

            }

        }
    }
}
